/* draws a rectangle at the top-left corner of the screen.
   The rectangle is 16 pixels wide and r0 pixels high. */

(KBDLOOP)
    @KBD     //  loop until key pressed
    D=M
    @KBDLOOP
    D;JEQ

    /* draws a rectangle at the top-left corner of the screen.
       The rectangle is 16 pixels wide and r0 pixels high. */

    @50      //  setup: rect will be 50 high
      D=A
    @R0
    //  setup: rect will be 50 high
    @R0
